{
  "name": "oma-digital-platform",
  "version": "2.0.0",
  "description": "Plateforme digitale OMA - Solutions IA pour PME sénégalaises",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:https": "next dev --experimental-https --hostname localhost --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "type-check:ci": "node scripts/type-check-ci.js",
    "type-coverage": "npx type-coverage --detail",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "npm run type-check && npm run lint && npm run format:check",
    "analyze": "ANALYZE=true next build",
    "sitemap": "node scripts/generate-sitemap.js",
    "seo-audit": "lighthouse --only-categories=seo,performance,accessibility --output=html --output-path=./seo-report.html",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "optimize-images": "node scripts/optimize-images.js",
    "build:analyze": "$env:ANALYZE='true'; npm run build",
    "lighthouse": "lighthouse http://localhost:3000 --output json --output-path ./tmp_rovodev_lighthouse-report.json --chrome-flags=\"--headless\"",
    "performance:budget": "node scripts/performance-budget.js",
    "optimize:images": "node scripts/optimize-images.js",
    "build:production": "npm run optimize:images && npm run sitemap && npm run build",
    "optimize-webp": "npx @squoosh/cli --webp '{\"quality\":80,\"method\":6}' public/images/*.webp",
    "generate-cert": "node generate-cert.js",
    "deploy:production": "node deploy-to-production.js",
    "deploy:dry-run": "node deploy-to-production.js --dry-run",
    "production:checklist": "node scripts/production-checklist-runner.js",
    "security:validate": "node scripts/security-validation.js",
    "performance:test": "node scripts/performance-load-test.js",
    "tts-stt:test": "node scripts/test-tts-stt-browsers.js",
    "monitoring:setup": "node scripts/setup-performance-monitoring.js",
    "migrate:production": "npx supabase db push",
    "test:tts-api": "node test-tts-api.js",
    "test:data-flow": "jest __tests__/data-flow-integrity.test.ts",
    "monitoring:health-check": "node scripts/health-check.js",
    "performance:review": "node scripts/db-performance-review.js",
    "migrate": "node scripts/run-migrations.js",
    "fix:circular": "node scripts/fix-circular-reference.js",
    "test:database": "node scripts/test-database-connection.js",
    "test:api": "node scripts/test-dashboard-api.js",
    "test:admin": "node scripts/comprehensive-admin-test.js",
    "security:audit": "node scripts/security-audit.js",
    "security:scan": "node scripts/security-secrets-scan.js",
    "security:check": "npm run security:scan && npm audit",
    "security:fix": "npm audit fix",
    "test:vertex-ai": "npx tsx scripts/test-vertex-ai.ts",
    "test:vertex-speech": "npx tsx scripts/test-vertex-ai-speech.ts",
    "list:models": "npx tsx scripts/list-available-models.ts",
    "setup:supabase": "npx tsx scripts/setup-supabase-schema.ts",
    "db:migrate": "npx tsx scripts/setup-supabase-schema.ts",
    "verify:all": "npm run test:vertex-ai && npm run test:vertex-speech && npm run type-check && npm run lint",
    "pre-commit": "npm run security:scan && npm run type-check && npm run lint"
  },
